}

@Article{Frohn2000,
  author    = {J. T. Frohn and H. F. Knapp and A. Stemmer},
  title     = {{True optical resolution beyond the {Rayleigh} limit achieved by standing wave illumination.}},
  journal   = {Proceedings of the National Academy of Sciences},
  year      = {2000},
  volume    = {97},
  number    = {13},
  pages     = {7232--6},
  month     = jun,
  issn      = {0027-8424},
