@Article{Fernandez-Dominguez2015,
  author    = {Antonio I. Fern{\'{a}}ndez-Dom{\'{\i}}nguez and Zhaowei Liu and John B. Pendry},
  title     = {Coherent Four-Fold Super-Resolution Imaging with Composite Photonic{\textendash}Plasmonic Structured Illumination},
  journal   = {{ACS} Photonics},
  year      = {2015},
  volume    = {2},
  number    = {3},
  pages     = {341--348},
  month     = mar,
  issn      = {23304022},
  abstract  = {We present a far-field super-resolution imaging scheme based on coherent scattering under a composite photonicâplasmonic structured illumination. The super-resolved image retrieval method, which involves the combination of 13 different diffraction-limited images of the specimen, is first developed within a Fourier optics framework. A. feasible implementation of this optical microscopy technique working at 465 nm is proposed and its point spread function is investigated using full electromagnetics calculations. The 4-fold super-resolution power of the scheme, able to resolve 60 nm feature sizes at the operating wavelength, is demonstrated against both Abbe's (imaging a single object) and (imaging two closely spaced objects) criteria., Rayleigh's},
